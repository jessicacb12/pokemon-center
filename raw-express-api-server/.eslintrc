{
    "env": {
        "es6": true,
        "node": true,
        "mocha": true
    },
    "parserOptions": {
        "ecmaVersion": 9
    },
    "extends": "airbnb-base",
    "rules": {
        "max-len": 0,
        "no-console": 0,
        "no-param-reassign": [
            "error",
            {
                "props": false
            }
        ],
        "no-underscore-dangle": 0,
        "no-use-before-define": [
            "error",
            {
                "functions": false
            }
        ],
        "quotes": [
            "error",
            "double"
        ]
    }
}